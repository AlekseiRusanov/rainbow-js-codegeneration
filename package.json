{
  "name": "rainbow-js-codegeneration",
  "version": "0.1.15",
  "description": "JavaScript based code-generation for Rainbow serialization format of Sitecore items",
  "keywords": [
    "sitecore",
    "rainbow",
    "generate",
    "unicorn",
    "codegeneration",
    "code generation",
    "gulpplugin"
  ],
  "engineStrict": true,
  "main": "./build/src/main.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "devDependencies": {
    "@types/jest": "~21.1.1",
    "@types/node": "~6.0.88",
    "jest": "~21.2.0",
    "rimraf": "~2.6.2",
    "tslint": "~5.7.0",
    "tslint-microsoft-contrib": "~5.0.1",
    "tsutils": "~2.9.0",
    "typescript": "~2.5.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --type-check --fix --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Alex Smagin <me@asmagin.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asmagin/rainbow-js-codegeneration.git"
  },
  "bugs": {
    "url": "https://github.com/asmagin/rainbow-js-codegeneration/issues"
  },
  "homepage": "https://github.com/asmagin/rainbow-js-codegeneration#readme",
  "dependencies": {
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "gulp-util": "^3.0.8",
    "handlebars": "^4.0.10",
    "jest-cli": "^21.2.1",
    "js-yaml": "^3.10.0",
    "loglevel": "^1.5.0",
    "through2": "^2.0.3",
    "tslib": "~1.7.1"
  }
}
